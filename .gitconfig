[credential]
	helper = /mnt/c/Program\\ Files\\ \\(x86\\)/Git/mingw64/libexec/git-core/git-credential-wincred.exe
[alias]
	co = checkout
[pull]
	rebase = false
[core]
	editor = code-insiders -w
[difftool "vscode"]
	cmd = code-insiders -w -d $LOCAL $REMOTE
[diff]
	tool = vscode
[mergetool "vscode"]
	cmd = code-insiders -w $MERGED
[merge]
	tool = vscode
